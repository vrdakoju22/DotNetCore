{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug"
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                    "path": "Logs\\.json",
                    "rollingInterval": "Day",
                    "shared": "true"
                }
            }
        ]
    }
}
